<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:mie="http://www.tcl.com/mie/schema/context"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
   http://www.tcl.com/mie/schema/context http://www.tcl.com/mie/schema/annotation-context.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
	default-autowire="byName">
	<context:property-placeholder location="classpath*:http.properties"  ignore-unresolvable="true" />
	
	<mie:scan base-package="com.tcl.mie"/>
</beans>
